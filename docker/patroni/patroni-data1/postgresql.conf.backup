# Do not edit this file manually!
# It will be overwritten by Patroni!
include 'postgresql.base.conf'

cluster_name = 'patroni'
hot_standby = 'on'
listen_addresses = '172.18.0.8'
max_connections = '100'
max_locks_per_transaction = '64'
max_prepared_transactions = '0'
max_replication_slots = '10'
max_wal_senders = '10'
max_worker_processes = '8'
port = '5432'
track_commit_timestamp = 'off'
unix_socket_directories = '/var/run/postgresql'
wal_keep_size = '128MB'
wal_level = 'replica'
wal_log_hints = 'on'
hba_file = '/var/lib/postgresql/patroni/main/pg_hba.conf'
ident_file = '/var/lib/postgresql/patroni/main/pg_ident.conf'

# recovery.conf
primary_conninfo = 'dbname=postgres user=replicator passfile=/tmp/pgpass0 host=172.18.0.7 port=5432 sslmode=prefer application_name=e42c1e46e5da gssencmode=prefer channel_binding=prefer sslnegotiation=postgres'
primary_slot_name = 'e42c1e46e5da'
recovery_target = ''
recovery_target_lsn = ''
recovery_target_name = ''
recovery_target_time = ''
recovery_target_timeline = 'latest'
recovery_target_xid = ''
